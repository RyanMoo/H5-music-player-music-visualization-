<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <header>
      <h1><%= title %></h1> 
      <ul>
        <li data-type="dot">Dot</li>
        <li data-type="column" class="show">Column</li>
      </ul>
      <p>
        Volume <input type="range" name="volume" id="volume" min="0" max="100" value="50">  
      </p>  
    </header>
    <div class="left">
      <ul id="list">
        <% music.forEach(function (name) {%>
          <li title='<%= name%>'><%= name%></li>          
        <%})%>
      </ul>
    </div>
    <div class="right" id="box"></div>
    <script src="/javascripts/MusicPlayer.js"></script>
    <script src="/javascripts/index.js"></script>
  </body>
</html>
